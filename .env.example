# Copy this to .env and fill in your values
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Semantic Scholar API key (raises rate limit from ~1 req/s to 10 req/s)
# Get one free at https://www.semanticscholar.org/product/api
S2_API_KEY=

# Optional: adjust model for cost/quality tradeoff
# Options: claude-opus-4-5, claude-sonnet-4-5, claude-haiku-4-5
ANTHROPIC_MODEL=claude-opus-4-5

# Optional: arxiv fetch config
ARXIV_MAX_RESULTS=20
ARXIV_SORT_BY=relevance  # relevance | lastUpdatedDate | submittedDate
